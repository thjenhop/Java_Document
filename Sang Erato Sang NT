#include <iostream>

using namespace std;


int check(const int &i);

int main()
{
    int n;
    cin >> n;
    if(n < 2)
    {
        return 0;
    }
    int i;
    for(i = 2; i <= n; i++)
    {
        if(check(i))
        {
            cout << i << "\n" ;
        }
    }


    return 0;
}
int check(const int &i)
{
    int j;
    for(j = 2; j < i; j++)
    {
        if(i % j == 0)
        {
            return 0;
        }
    }
    return 1;
}
